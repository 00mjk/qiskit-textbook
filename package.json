{
  "name": "sw-ibmq-stats",
  "version": "1.0.0",
  "description": "Jobs to synchronize the monitoring db",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "echo \"BASE_URL=$BASE_URL\" && npm run clean && run-p build:*",
    "build:docs": "sphinx-build -b html documentation dist/documentation && ts-node src/postprocess/postprocess.ts",
    "build:ts": "babel src --extensions=\".ts,.tsx\" --out-dir dist/server",
    "build:css": "node-sass --include-path ./node_modules/ src/css -o dist/css",
    "build:images": "copyfiles --follow -u 1 \"documentation/**/*.{jpeg,jpg,png,svg}\" dist/documentation",
    "dev": "yarn build:css && yarn build:ts && run-p dev:docs dev:images",
    "dev:docs": "nodemon --exec \"yarn build:docs && NODE_ENV=development yarn start\" --ext rst,md,py,ipynb -w documentation --ignore documentation/api/**/* --ignore documentation/stubs",
    "dev:server": "NODE_ENV=development ELASTIC_URL=http://localhost:9200 nodemon --exec \"ts-node src/index.ts\" --ext ts,json,tsx -w src -w dist",
    "dev:css": "nodemon --exec \"npm run build:css\" --ext scss -w src/css",
    "dev:images": "nodemon --exec \"npm run build:images\" --ext jpeg,jpg,png,svg -w documentation",
    "type-check": "tsc --noEmit",
    "start": "node dist/server/index.js",
    "test": "NODE_ENV=test TC=UTC ELASTIC_URL=elastic jest src scripts",
    "lint": "yarn type-check"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@carbon/elements": "^10.7.0",
    "@sindresorhus/slugify": "^0.9.1",
    "@types/cheerio": "^0.22.15",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/elasticsearch": "^5.0.36",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/lunr": "^2.3.2",
    "@types/memoizee": "^0.4.3",
    "@types/mkdirp": "^0.5.2",
    "@types/mongodb": "^3.3.14",
    "@types/ms": "^0.7.31",
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/sbd": "^1.0.1",
    "@types/socket.io": "^2.1.4",
    "babel-plugin-transform-typescript-metadata": "^0.2.2",
    "bson": "^4.0.2",
    "cheerio": "^1.0.0-rc.3",
    "copyfiles": "^2.2.0",
    "globby": "^10.0.1",
    "jest": "^24.9.0",
    "mathjax-node-page": "^3.2.0",
    "mkdirp": "^0.5.1",
    "mongodb": "^3.4.0",
    "node-pandoc": "^0.3.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "slugify": "^1.3.6",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "add": "^2.0.6",
    "axios": "^0.19.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "elasticsearch": "^16.6.0",
    "express": "^4.17.1",
    "is-absolute-url": "^3.0.3",
    "lodash": "^4.17.15",
    "lunr": "^2.3.8",
    "memoizee": "^0.4.14",
    "ms": "^2.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sbd": "^1.0.16",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1"
  }
}
